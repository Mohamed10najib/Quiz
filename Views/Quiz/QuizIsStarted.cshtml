@{
    Dictionary<User, int> userScores = ViewBag.userScores;
    
}
<p>Quiz Status:</p>
@if (userScores != null && userScores.Count > 0)
{
    @foreach (var pair in userScores)
    {
        <p>
            <span>@pair.Key.Username</span>
            <span>Score: @pair.Value</span>
        </p>
    }
}
else
{
    <p>The quiz has not started yet.</p>
}